{
  "name": "quantumpoly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run check-middleware && npm run update-middleware && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:middleware": "jest --testPathPattern=middleware",
    "test:redirects": "ts-node scripts/test-redirects.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "update-middleware": "node scripts/updateMiddleware.js",
    "check-middleware": "node scripts/check-middleware-integrity.js",
    "visualize-middleware": "ts-node scripts/visualize-middleware-matcher.ts",
    "visualize-redirects": "ts-node -r tsconfig-paths/register src/utils/dev/redirectVisualizer.ts",
    "intent-audit": "node scripts/intent-audit.js",
    "visualize-intent": "ts-node scripts/visualize-intent.ts",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "next": "^14.2.30",
    "next-intl": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "20.17.25",
    "@types/react": "18.3.19",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
