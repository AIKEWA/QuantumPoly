{
  "name": "quantumpoly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:write": "prettier --write .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:integration": "jest __tests__/integration --passWithNoTests",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "echo 'Storybook a11y tests: Use npm run storybook and check addon-a11y panel for accessibility violations'",
    "prepare": "husky install",
    "docs:props": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generate-props-md.ts",
    "validate": "npm run lint && npm run type-check && npm run test",
    "ci": "npm run validate && npm run build && npm run build-storybook"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@lhci/cli": "^0.13.0",
    "@storybook/addon-a11y": "^7.6.20",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/blocks": "^7.5.0",
    "@storybook/nextjs": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@storybook/test": "^7.6.20",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.6",
    "@types/node": "20.17.25",
    "@types/react": "18.3.19",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "globby": "^14.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "markdown-table": "^3.0.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^7.5.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
